        <!-- bootstrap tagsinput -->
        <script src="{{asset('js/bootstrap-tagsinput/src/bootstrap-tagsinput.js')}}"></script>

        <!-- bootstrap colorpicker -->
        <script src="{{asset('js/bootstrap-colorpicker/dist/js/bootstrap-colorpicker.min.js')}}"></script>

        <script src="{{asset('js/drag-and-drop.js')}}"></script>
                                              
        <script src="{{asset('js/my.js')}}"></script>
        
        <script type="text/javascript">

            $(document).ready( function () {
                $('[data-toggle="popover"]').popover();

                $('[data-toggle="tooltip"]').tooltip();

                $(".select-entity").each(function(){
                    var id = $(this).data('id');
                    $(this).val(id);
                });
            });

            $('.text-upper-case').keyup(function(e){
                $(this).val($(this).val().toUpperCase());
            })

            $('.text-color').colorpicker({
                format: "hex"
            });

            $('.check-all').click(function(e) {
                e.preventDefault();
                var name = $(this).data('name');
                $("input[name='"+name+"']").each(function(){
                    $(this).prop('checked',true);
                });
            });

            $('.check-none').click(function(e) {
                e.preventDefault();
                var name = $(this).data('name');
                $("input[name='"+name+"']").each(function(){
                    $(this).prop('checked',false);
                });
            });




        </script>
        {% block javascripts %}{% endblock %}
    </body>
</html>
